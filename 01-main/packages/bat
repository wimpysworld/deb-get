DEFVER=1
ARCHS_SUPPORTED="amd64 arm64 armhf"
get_github_releases "sharkdp/bat" "latest"
if [ "${ACTION}" != "prettylist" ]; then
    URL=$(grep "browser_download_url.*${HOST_ARCH}\.deb\"" "${CACHE_FILE}" | grep -m 1 -v musl | cut -d '"' -f 4)
    VERSION_PUBLISHED=$(cut -d '_' -f 2 <<< "${URL}")
fi
PRETTY_NAME="bat"
WEBSITE="https://github.com/sharkdp/bat"
SUMMARY="A 'cat' clone with wings."

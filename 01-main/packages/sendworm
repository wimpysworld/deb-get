DEFVER=1
ARCHS_SUPPORTED="amd64 arm64"
CODENAMES_SUPPORTED="bookworm trixie sid jammy noble questing"
get_github_releases "ubuntuegor/sendworm" "latest"
if [ "${ACTION}" != prettylist ]; then
    URL="$(grep -m 1 "browser_download_url.*${HOST_ARCH}.*\.deb\"" "${CACHE_FILE}" | cut -d '"' -f 4)"
    VERSION_PUBLISHED="$(cut -d '/' -f 8 <<< "${URL//v/}")"
fi
PRETTY_NAME="Sendworm"
WEBSITE="https://github.com/ubuntuegor/sendworm"
SUMMARY="Cross-platform desktop app to share files using Magic Wormhole."

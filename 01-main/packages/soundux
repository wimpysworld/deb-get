if [ "${ACTION}" != "prettylist" ]; then
    URL=$(grep "browser_download_url.*.deb" "${CACHE_FILE}" | cut -d '"' -f 4)
    VERSION_PUBLISHED=$(cut -d '/' -f 8 <<< "${URL}" | tr -d v)
fi

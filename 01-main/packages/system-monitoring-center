if [ "${ACTION}" != "prettylist" ]; then
    URL=$(grep "browser_download_url.*all\.deb\"" "${CACHE_FILE}" | head -n1 | cut -d'"' -f4)
    VERSION_PUBLISHED="$(echo "${URL}" |grep -o -E '[[:digit:]]+\.[[:digit:]]+\.[[:digit:]]+' |sort -u)"
fi

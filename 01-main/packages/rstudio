DEFVER=1
get_website "https://posit.co/download/rstudio-desktop/"
if [ "${ACTION}" != "prettylist" ]; then
    case "${UPSTREAM_CODENAME}" in
        focal|buster|bullseye)
            URL=$( grep -e "focal/.*amd64.deb" "${CACHE_FILE}" | grep -m 1 -v tar.gz | cut -d '"' -f 4)
            ;;
        *)
            URL=$( grep -e "jammy/.*amd64.deb" "${CACHE_FILE}" | grep -m 1 -v tar.gz | cut -d '"' -f 4)
            ;;
    esac
    VERSION_PUBLISHED=$(cut -d '-' -f 2-3 <<< "${URL}" | tr - +)
fi
PRETTY_NAME="RStudio"
WEBSITE="https://posit.co/"
#https://posit.co/products/open-source/rstudio/
SUMMARY="Professional software for data science teams."

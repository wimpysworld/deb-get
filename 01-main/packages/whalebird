if [ "${ACTION}" != "prettylist" ]; then
    URL=$(grep "browser_download_url.*64\.deb\"" "${CACHE_FILE}" | grep -m 1 -E -v '(alpha|beta|-rc)\.' | cut -d '"' -f 4)
    VERSION_PUBLISHED=$(cut -d '/' -f 8 <<< "${URL//v/}")
fi

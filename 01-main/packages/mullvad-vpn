DEFVER=1
ARCHS_SUPPORTED="amd64 arm64"
get_github_releases "mullvad/mullvadvpn-app"
if [ "${ACTION}" != "prettylist" ]; then
    URL=$(grep "browser_download_url.*${HOST_ARCH}\.deb\"" "${CACHE_FILE}" | grep -m 1 -v -e nightly -e beta -e alpha| cut -d '"' -f 4)
    VERSION_PUBLISHED=$(cut -d '/' -f 8 <<< "${URL}")
fi
PRETTY_NAME="Mullvad VPN Client"
WEBSITE="https://mullvad.net/"
SUMMARY="VPN Service based in Sweden that does not collect user data."

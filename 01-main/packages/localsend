if [ "${ACTION}" != prettylist ]; then
    URL=$(grep -m1 -o "\"browser_download_url\":[[:space:]]*\"[^\"]*x86-64\.deb\"" "${CACHE_FILE}")
    URL=${URL%\"}; URL=${URL##*\"}
    VERSION_PUBLISHED=${URL##*/download/v}
    VERSION_PUBLISHED=${VERSION_PUBLISHED%%/*}
fi

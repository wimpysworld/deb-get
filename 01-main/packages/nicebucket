DEFVER=1
ARCHS_SUPPORTED="amd64"
CODENAMES_SUPPORTED="buster bullseye bookworm trixie sid focal jammy lunar mantic noble"
get_github_releases "nicebucket-org/nicebucket" "latest"
if [ "${ACTION}" != prettylist ]; then
    URL="$(grep -m 1 "browser_download_url.*\.deb\"" "${CACHE_FILE}" | cut -d'"' -f4)"
    VERSION_PUBLISHED="$(cut -d'/' -f8 <<< "${URL}")"
fi

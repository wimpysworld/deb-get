DEFVER=1
ARCHS_SUPPORTED="amd64 armhf"
get_github_releases "libretro/ludo"
if [ "${ACTION}" != "prettylist" ]; then
    URL=$(grep "browser_download_url.*${HOST_ARCH}\.deb\"" "${CACHE_FILE}" | grep -m 1 -v -e altui | cut -d '"' -f 4)
    VERSION_PUBLISHED=$(cut -d '/' -f 8 <<< "${URL/v/}")
fi
PRETTY_NAME="Ludo"
WEBSITE="https://ludo.libretro.com/"
SUMMARY="A minimalist frontend for emulators."

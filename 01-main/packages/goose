if [ "${ACTION}" != prettylist ]; then
    URL="$(grep -m 1 "browser_download_url.*\.deb\"" "${CACHE_FILE}" | cut -d '"' -f 4)"
    VERSION_PUBLISHED="$(cut -d '_' -f 2 <<< "${URL}")"
fi

    URL=$(grep -m1 -o -E "https://download\.smartgit\.dev/smartgit/smartgit-[[:digit:]]+_[[:digit:]]+_[[:digit:]]+-linux_$HOST_ARCH.deb" "${CACHE_FILE}")
    VERSION_PUBLISHED=$(cut -d '-' -f 2 <<< "${URL//_/.}")
fi

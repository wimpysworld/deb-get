if [ "${ACTION}" != prettylist ]; then
    URL="$(grep -m 1 "browser_download_url.*${HOST_ARCH//amd64/linux-debian}.*\.deb\"" "${CACHE_FILE}" | cut -d '"' -f 4)"
    VERSION_PUBLISHED="$(cut -d '/' -f 8 <<< "${URL//v/}")"
fi

if [ "${ACTION}" != "prettylist" ]; then
    URL=$(grep "browser_download_url.*${HOST_ARCH}_linux\.deb\"" "${CACHE_FILE}" | cut -d '"' -f 4)
    VERSION_PUBLISHED=$(cut -d '_' -f 2 <<< "${URL}")
fi

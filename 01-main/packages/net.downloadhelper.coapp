DEFVER=1
ARCHS_SUPPORTED="amd64 arm64"
get_github_releases "aclap-dev/vdhcoapp" "latest"
if [ "${ACTION}" != prettylist ]; then
    URL="$(grep -m 1 "browser_download_url.*${HOST_CPU}\.deb\"" "${CACHE_FILE}" | cut -d '"' -f 4)"
    VERSION_PUBLISHED="$(sed -E 's|.*/download/v([^/]*)/.*|\1|' <<< "${URL}")"
fi
PRETTY_NAME="DownloadHelper CoApp"
WEBSITE="https://github.com/aclap-dev/vdhcoapp"
SUMMARY="Companion application for Video DownloadHelper browser add-on."
